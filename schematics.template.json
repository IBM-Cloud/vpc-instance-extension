{
    "name": $TF_VAR_basename,
    "type": [
      "terraform_v0.12"
    ],
    "description": "vpc for flow logs",
    "template_repo": {
      "url": "https://github.com/IBM-Cloud/vpc-tutorials",
      "branch": "vsi"
    },
    "template_data": [
      {
        "folder": "vpc-vsi/tf",
        "type": "terraform_v0.12",
        "variablestore": [
          {
            "name": "vpc_name",
            "secure": false,
            "value": $TF_VAR_basename,
            "type": "string"
          },
          {
            "name": "basename",
            "secure": false,
            "value": $TF_VAR_basename,
            "type": "string"
          },
          {
            "name": "ssh_keyname",
            "secure": false,
            "value": $TF_VAR_ssh_key_name,
            "type": "string"
          }
        ]
      }
    ]
  }